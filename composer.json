{
    "name": "mzaman/laravel-api-response",
    "description": "A flexible and scalable API response handling package for Laravel",
    "version": "2.1.0",
    "type": "library",
    "homepage": "https://masudzaman.com",
    "keywords": [
        "laravel",
        "api",
        "response",
        "json",
        "rest"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Masud Zaman",
            "email": "masud.zmn@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MasudZaman\\LaravelApiResponse\\": "src/"
        },
        "files": [
            "src/Helpers/helper.php"
        ]
    },
    "require": {
        "php": "^7.2|^7.3|^8.0|^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MasudZaman\\LaravelApiResponse\\Providers\\LaravelApiResponseServiceProvider",
                "MasudZaman\\LaravelApiResponse\\Providers\\ApiExceptionHandlerServiceProvider"
            ],
            "aliases": {
                "ApiResponse": "MasudZaman\\LaravelApiResponse\\Facades\\ApiResponse"
            }
        }
    }
}