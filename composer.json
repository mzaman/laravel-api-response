{
    "name": "mzaman/laravel-api-response",
    "description": "A flexible and scalable API response handling package for Laravel",
    "version": "1.0.0",
    "type": "library",
    "keywords": [
        "laravel",
        "api",
        "response",
        "json",
        "rest"
    ],
    "autoload": {
        "psr-4": {
            "MasudZaman\\LaravelApiResponse\\": "src/"
        },
        "files": [
            "src/Helpers/helper.php"
        ]
    },
    "require": {
        "php": "^7.3|^8.0|^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MasudZaman\\LaravelApiResponse\\LaravelApiResponseServiceProvider"
            ]
        }
    }
}